<html lang="en">
    <body>
        <p>
            Number of occurrences of events for window <b>{{ window }}</b> between {{ datetime_from }} and {{ datetime_generated }}
        </p>
        <table>
            {% for comb in statistics %}
                <tr>
                    <th class="c1">{{ comb.kind }}</th>
                    <th class="c2">{{ comb.reason }}</th>
                    <th class="c3">Namespace</th>
                    <th class="c4">Value</th>
                </tr>
                {% for line in comb.ns %}
                <tr>
                    <td class="c1"><span/></td>
                    <td class="c2"><span/></td>
                    <td class="c3">{{ line.name }}</td>
                    <td class="c4">{{ line.value }}</td>
                </tr>
                {% endfor %}
            {% endfor %}
        </table>
    </body>
</html>